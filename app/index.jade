doctype html
html
  head
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    link(rel='stylesheet' href='vendor/font-awesome/css/font-awesome.min.css')
    link(rel='stylesheet' href='index.css' type='text/css')
    title COHORT 8 is Great
  body(ng-app='ch8App')
    #nav-header.container-fluid
      nav.navbar.navbar-default.navbar-fixed-top
        .container-fluid
          // Brand and toggle get grouped for better mobile display
          .navbar-header
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#nss-mobile-nav')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(ui-sref='home').cohort-title
              img.nss_logo(src='images/logos/nss-8_final_2_1.png')
          // Collect the nav links, forms, and other content for toggling
          #nss-mobile-nav.collapse.navbar-collapse
            ul.nav.navbar-nav.navbar-right
              li.active: a(ui-sref='home'): i.fa.fa-home &nbsp; Home
              li: a(ui-sref='nss' data-toggle='collapse' data-target='#navbar-collapse-1'): i.fa.fa-desktop &nbsp; NSS Program
              li: a(ui-sref='students'): i.fa.fa-users &nbsp; Meet the Team
              li: a(ui-sref='contact'): i.fa.fa-envelope-o &nbsp; Contact Us
          // /.navbar-collapse
        // /.container-fluid


  div(ui-view)


    script(src='vendor/jquery/dist/jquery.js')
    script(src='vendor/bootstrap-sass/assets/javascripts/bootstrap.js')
    script(src='vendor/lodash/lodash.js')
    script(src='vendor/angular/angular.js')
    script(src='vendor/angular-ui-router/release/angular-ui-router.js')
    script(src='index.js')
    script(src='models/students.js')
    script(src='views/students.js')
    script(src='models/skills.js')
    script(src='views/skills.js')
